
@keyframes em-fade-in
  0%
    opacity: 0
  100%
    opacity: 1


@keyframes em-fade-out
  100%
    opacity: 0


@keyframes em-slide-from-right
  0%
    transform: translateX(-100%) translateZ(0)
  100%
    transform: translateX(0) translateZ(0)


@keyframes em-slide-to-right
  100%
    transform: translateX(-100%) translateZ(0)


@keyframes em-disappear-below
  0%
    transform: translateZ(0)

@keyframes em-appear-from-top
  0%
    transform: translateZ(0)
  100%
    transform: translateZ(0)


@keyframes em-dialog-root-enter

  0%
    transform: translate3d(0, 50px, 0) scale3d(1.1, 1.1, 1)

  100%
    transform: translate3d(0, 0, 0) scale3d(1, 1, 1)

@keyframes em-dialog-root-leave

  0%
    transform: translate3d(0, 0, 0) scale3d(1, 1, 1)

  100%
    transform: translate3d(0, -50px, 0) scale3d(0.9, 0.9, 1)
